{
  "version": 2,
  "projects": {
    "button": "libs/button",
    "naf-x": "apps/naf-x",
    "styles": "libs/styles"
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "karma",
      "e2eTestRunner": "none",
      "standaloneConfig": true,
      "skipTests": true
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "none",
      "buildable": true,
      "publishable": true,
      "standaloneConfig": true
    },
    "@schematics/angular:component": {
      "style": "scss",
      "export": true,
      "skipTests": true,
      "changeDetection": "OnPush",
      "flat": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "naf-x"
}
